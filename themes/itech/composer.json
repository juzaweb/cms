{
    "name": "juzaweb/itech",
    "type": "juzaweb-theme",
    "description": "Juzaweb CMS theme Itech",
    "keywords": [],
    "license": "MIT",
    "homepage": "https://juzaweb.com",
    "require": {
        "php": "^8.2",
        "juzaweb/ads-manager": "^2.0",
        "juzaweb/blog": "^1.0",
        "juzaweb/core": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\Themes\\Itech\\": "src/",
            "Juzaweb\\Themes\\Itech\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Juzaweb\\Themes\\Itech\\Tests\\": "tests/"
        }
    }
}
