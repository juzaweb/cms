{
    "name": "juzaweb/cms",
    "description": "Juzaweb CMS is a Content Management System (CMS) developed based on Laravel Framework and web platform whose sole purpose is to make your development workflow simple again. Project develop by Juzaweb",
    "keywords": [
        "juzaweb cms",
        "juzaweb.com cms",
        "juzaweb.com",
        "laravel cms",
        "cms like wordpress",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "https://juzaweb.com",
    "require": {
        "php": "^8.0|^8.1",
        "intervention/image": "^2.5",
        "juzaweb/config": "^2.2",
        "spatie/image-optimizer": "^1.4",
        "juzaweb/laravel-chunk-upload": "^1.5",
        "arcanedev/log-viewer": "^9.0",
        "darkaonline/l5-swagger": "^8.0",
        "rcrowe/twigbridge": "^0.14",
        "spatie/laravel-translation-loader": "@dev",
        "rennokki/laravel-eloquent-query-cache": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "6.0 | 7.0 | 7.5 | 8.4 | ^8.5"
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\": "src/"
        },
        "files": [
            "src/Helpers/define.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juzaweb\\Providers\\CmsServiceProvider"
            ]
        }
    }
}
